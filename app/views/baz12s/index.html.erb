<p id="notice"><%= notice %></p>

<h1>Baz12s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz12s.each do |baz12| %>
      <tr>
        <td><%= baz12.foo1 %></td>
        <td><%= baz12.foo2 %></td>
        <td><%= baz12.foo3 %></td>
        <td><%= baz12.foo4 %></td>
        <td><%= baz12.foo5 %></td>
        <td><%= link_to 'Show', baz12 %></td>
        <td><%= link_to 'Edit', edit_baz12_path(baz12) %></td>
        <td><%= link_to 'Destroy', baz12, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

