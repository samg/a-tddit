<p id="notice"><%= notice %></p>

<h1>Foo48s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo48s.each do |foo48| %>
      <tr>
        <td><%= foo48.foo1 %></td>
        <td><%= foo48.foo2 %></td>
        <td><%= foo48.foo3 %></td>
        <td><%= foo48.foo4 %></td>
        <td><%= foo48.foo5 %></td>
        <td><%= link_to 'Show', foo48 %></td>
        <td><%= link_to 'Edit', edit_foo48_path(foo48) %></td>
        <td><%= link_to 'Destroy', foo48, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

