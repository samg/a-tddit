<p id="notice"><%= notice %></p>

<h1>Baz88s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz88s.each do |baz88| %>
      <tr>
        <td><%= baz88.foo1 %></td>
        <td><%= baz88.foo2 %></td>
        <td><%= baz88.foo3 %></td>
        <td><%= baz88.foo4 %></td>
        <td><%= baz88.foo5 %></td>
        <td><%= link_to 'Show', baz88 %></td>
        <td><%= link_to 'Edit', edit_baz88_path(baz88) %></td>
        <td><%= link_to 'Destroy', baz88, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

