<p id="notice"><%= notice %></p>

<h1>Baz50s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz50s.each do |baz50| %>
      <tr>
        <td><%= baz50.foo1 %></td>
        <td><%= baz50.foo2 %></td>
        <td><%= baz50.foo3 %></td>
        <td><%= baz50.foo4 %></td>
        <td><%= baz50.foo5 %></td>
        <td><%= link_to 'Show', baz50 %></td>
        <td><%= link_to 'Edit', edit_baz50_path(baz50) %></td>
        <td><%= link_to 'Destroy', baz50, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz50', new_baz50_path %>
