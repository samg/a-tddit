<p id="notice"><%= notice %></p>

<h1>Bar75s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar75s.each do |bar75| %>
      <tr>
        <td><%= bar75.foo1 %></td>
        <td><%= bar75.foo2 %></td>
        <td><%= bar75.foo3 %></td>
        <td><%= bar75.foo4 %></td>
        <td><%= bar75.foo5 %></td>
        <td><%= link_to 'Show', bar75 %></td>
        <td><%= link_to 'Edit', edit_bar75_path(bar75) %></td>
        <td><%= link_to 'Destroy', bar75, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar75', new_bar75_path %>
