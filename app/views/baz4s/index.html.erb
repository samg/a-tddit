<p id="notice"><%= notice %></p>

<h1>Baz4s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz4s.each do |baz4| %>
      <tr>
        <td><%= baz4.foo1 %></td>
        <td><%= baz4.foo2 %></td>
        <td><%= baz4.foo3 %></td>
        <td><%= baz4.foo4 %></td>
        <td><%= baz4.foo5 %></td>
        <td><%= link_to 'Show', baz4 %></td>
        <td><%= link_to 'Edit', edit_baz4_path(baz4) %></td>
        <td><%= link_to 'Destroy', baz4, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz4', new_baz4_path %>
