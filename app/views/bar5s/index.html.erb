<p id="notice"><%= notice %></p>

<h1>Bar5s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar5s.each do |bar5| %>
      <tr>
        <td><%= bar5.foo1 %></td>
        <td><%= bar5.foo2 %></td>
        <td><%= bar5.foo3 %></td>
        <td><%= bar5.foo4 %></td>
        <td><%= bar5.foo5 %></td>
        <td><%= link_to 'Show', bar5 %></td>
        <td><%= link_to 'Edit', edit_bar5_path(bar5) %></td>
        <td><%= link_to 'Destroy', bar5, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar5', new_bar5_path %>
