<p id="notice"><%= notice %></p>

<h1>Foo55s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo55s.each do |foo55| %>
      <tr>
        <td><%= foo55.foo1 %></td>
        <td><%= foo55.foo2 %></td>
        <td><%= foo55.foo3 %></td>
        <td><%= foo55.foo4 %></td>
        <td><%= foo55.foo5 %></td>
        <td><%= link_to 'Show', foo55 %></td>
        <td><%= link_to 'Edit', edit_foo55_path(foo55) %></td>
        <td><%= link_to 'Destroy', foo55, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foo55', new_foo55_path %>
