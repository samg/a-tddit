<p id="notice"><%= notice %></p>

<h1>Bar11s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar11s.each do |bar11| %>
      <tr>
        <td><%= bar11.foo1 %></td>
        <td><%= bar11.foo2 %></td>
        <td><%= bar11.foo3 %></td>
        <td><%= bar11.foo4 %></td>
        <td><%= bar11.foo5 %></td>
        <td><%= link_to 'Show', bar11 %></td>
        <td><%= link_to 'Edit', edit_bar11_path(bar11) %></td>
        <td><%= link_to 'Destroy', bar11, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar11', new_bar11_path %>
