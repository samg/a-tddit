<p id="notice"><%= notice %></p>

<h1>Bar36s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar36s.each do |bar36| %>
      <tr>
        <td><%= bar36.foo1 %></td>
        <td><%= bar36.foo2 %></td>
        <td><%= bar36.foo3 %></td>
        <td><%= bar36.foo4 %></td>
        <td><%= bar36.foo5 %></td>
        <td><%= link_to 'Show', bar36 %></td>
        <td><%= link_to 'Edit', edit_bar36_path(bar36) %></td>
        <td><%= link_to 'Destroy', bar36, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar36', new_bar36_path %>
