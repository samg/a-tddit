<p id="notice"><%= notice %></p>

<h1>Bar2s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar2s.each do |bar2| %>
      <tr>
        <td><%= bar2.foo1 %></td>
        <td><%= bar2.foo2 %></td>
        <td><%= bar2.foo3 %></td>
        <td><%= bar2.foo4 %></td>
        <td><%= bar2.foo5 %></td>
        <td><%= link_to 'Show', bar2 %></td>
        <td><%= link_to 'Edit', edit_bar2_path(bar2) %></td>
        <td><%= link_to 'Destroy', bar2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar2', new_bar2_path %>
