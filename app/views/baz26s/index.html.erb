<p id="notice"><%= notice %></p>

<h1>Baz26s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz26s.each do |baz26| %>
      <tr>
        <td><%= baz26.foo1 %></td>
        <td><%= baz26.foo2 %></td>
        <td><%= baz26.foo3 %></td>
        <td><%= baz26.foo4 %></td>
        <td><%= baz26.foo5 %></td>
        <td><%= link_to 'Show', baz26 %></td>
        <td><%= link_to 'Edit', edit_baz26_path(baz26) %></td>
        <td><%= link_to 'Destroy', baz26, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz26', new_baz26_path %>
