<p id="notice"><%= notice %></p>

<h1>Bar41s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar41s.each do |bar41| %>
      <tr>
        <td><%= bar41.foo1 %></td>
        <td><%= bar41.foo2 %></td>
        <td><%= bar41.foo3 %></td>
        <td><%= bar41.foo4 %></td>
        <td><%= bar41.foo5 %></td>
        <td><%= link_to 'Show', bar41 %></td>
        <td><%= link_to 'Edit', edit_bar41_path(bar41) %></td>
        <td><%= link_to 'Destroy', bar41, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar41', new_bar41_path %>
