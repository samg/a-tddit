<p id="notice"><%= notice %></p>

<h1>Bar48s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar48s.each do |bar48| %>
      <tr>
        <td><%= bar48.foo1 %></td>
        <td><%= bar48.foo2 %></td>
        <td><%= bar48.foo3 %></td>
        <td><%= bar48.foo4 %></td>
        <td><%= bar48.foo5 %></td>
        <td><%= link_to 'Show', bar48 %></td>
        <td><%= link_to 'Edit', edit_bar48_path(bar48) %></td>
        <td><%= link_to 'Destroy', bar48, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar48', new_bar48_path %>
