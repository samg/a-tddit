<p id="notice"><%= notice %></p>

<h1>Bar50s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar50s.each do |bar50| %>
      <tr>
        <td><%= bar50.foo1 %></td>
        <td><%= bar50.foo2 %></td>
        <td><%= bar50.foo3 %></td>
        <td><%= bar50.foo4 %></td>
        <td><%= bar50.foo5 %></td>
        <td><%= link_to 'Show', bar50 %></td>
        <td><%= link_to 'Edit', edit_bar50_path(bar50) %></td>
        <td><%= link_to 'Destroy', bar50, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar50', new_bar50_path %>
