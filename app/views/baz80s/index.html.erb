<p id="notice"><%= notice %></p>

<h1>Baz80s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz80s.each do |baz80| %>
      <tr>
        <td><%= baz80.foo1 %></td>
        <td><%= baz80.foo2 %></td>
        <td><%= baz80.foo3 %></td>
        <td><%= baz80.foo4 %></td>
        <td><%= baz80.foo5 %></td>
        <td><%= link_to 'Show', baz80 %></td>
        <td><%= link_to 'Edit', edit_baz80_path(baz80) %></td>
        <td><%= link_to 'Destroy', baz80, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz80', new_baz80_path %>
