<p id="notice"><%= notice %></p>

<h1>Bar3s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar3s.each do |bar3| %>
      <tr>
        <td><%= bar3.foo1 %></td>
        <td><%= bar3.foo2 %></td>
        <td><%= bar3.foo3 %></td>
        <td><%= bar3.foo4 %></td>
        <td><%= bar3.foo5 %></td>
        <td><%= link_to 'Show', bar3 %></td>
        <td><%= link_to 'Edit', edit_bar3_path(bar3) %></td>
        <td><%= link_to 'Destroy', bar3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar3', new_bar3_path %>
