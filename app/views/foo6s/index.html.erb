<p id="notice"><%= notice %></p>

<h1>Foo6s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo6s.each do |foo6| %>
      <tr>
        <td><%= foo6.foo1 %></td>
        <td><%= foo6.foo2 %></td>
        <td><%= foo6.foo3 %></td>
        <td><%= foo6.foo4 %></td>
        <td><%= foo6.foo5 %></td>
        <td><%= link_to 'Show', foo6 %></td>
        <td><%= link_to 'Edit', edit_foo6_path(foo6) %></td>
        <td><%= link_to 'Destroy', foo6, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

