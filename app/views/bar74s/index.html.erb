<p id="notice"><%= notice %></p>

<h1>Bar74s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar74s.each do |bar74| %>
      <tr>
        <td><%= bar74.foo1 %></td>
        <td><%= bar74.foo2 %></td>
        <td><%= bar74.foo3 %></td>
        <td><%= bar74.foo4 %></td>
        <td><%= bar74.foo5 %></td>
        <td><%= link_to 'Show', bar74 %></td>
        <td><%= link_to 'Edit', edit_bar74_path(bar74) %></td>
        <td><%= link_to 'Destroy', bar74, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

