<p id="notice"><%= notice %></p>

<h1>Bar49s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar49s.each do |bar49| %>
      <tr>
        <td><%= bar49.foo1 %></td>
        <td><%= bar49.foo2 %></td>
        <td><%= bar49.foo3 %></td>
        <td><%= bar49.foo4 %></td>
        <td><%= bar49.foo5 %></td>
        <td><%= link_to 'Show', bar49 %></td>
        <td><%= link_to 'Edit', edit_bar49_path(bar49) %></td>
        <td><%= link_to 'Destroy', bar49, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar49', new_bar49_path %>
