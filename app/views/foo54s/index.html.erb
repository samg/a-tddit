<p id="notice"><%= notice %></p>

<h1>Foo54s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo54s.each do |foo54| %>
      <tr>
        <td><%= foo54.foo1 %></td>
        <td><%= foo54.foo2 %></td>
        <td><%= foo54.foo3 %></td>
        <td><%= foo54.foo4 %></td>
        <td><%= foo54.foo5 %></td>
        <td><%= link_to 'Show', foo54 %></td>
        <td><%= link_to 'Edit', edit_foo54_path(foo54) %></td>
        <td><%= link_to 'Destroy', foo54, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

