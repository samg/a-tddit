<p id="notice"><%= notice %></p>

<h1>Foo4s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo4s.each do |foo4| %>
      <tr>
        <td><%= foo4.foo1 %></td>
        <td><%= foo4.foo2 %></td>
        <td><%= foo4.foo3 %></td>
        <td><%= foo4.foo4 %></td>
        <td><%= foo4.foo5 %></td>
        <td><%= link_to 'Show', foo4 %></td>
        <td><%= link_to 'Edit', edit_foo4_path(foo4) %></td>
        <td><%= link_to 'Destroy', foo4, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foo4', new_foo4_path %>
