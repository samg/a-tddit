<p id="notice"><%= notice %></p>

<h1>Bar71s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar71s.each do |bar71| %>
      <tr>
        <td><%= bar71.foo1 %></td>
        <td><%= bar71.foo2 %></td>
        <td><%= bar71.foo3 %></td>
        <td><%= bar71.foo4 %></td>
        <td><%= bar71.foo5 %></td>
        <td><%= link_to 'Show', bar71 %></td>
        <td><%= link_to 'Edit', edit_bar71_path(bar71) %></td>
        <td><%= link_to 'Destroy', bar71, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar71', new_bar71_path %>
