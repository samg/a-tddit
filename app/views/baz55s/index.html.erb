<h1>Baz55s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz55s.each do |baz55| %>
      <tr>
        <td><%= baz55.foo1 %></td>
        <td><%= baz55.foo2 %></td>
        <td><%= baz55.foo3 %></td>
        <td><%= baz55.foo4 %></td>
        <td><%= baz55.foo5 %></td>
        <td><%= link_to 'Show', baz55 %></td>
        <td><%= link_to 'Edit', edit_baz55_path(baz55) %></td>
        <td><%= link_to 'Destroy', baz55, method: :delete, data: { confirm: 'Are you sure?' } %></td>
