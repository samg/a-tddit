<p id="notice"><%= notice %></p>

<h1>Bar85s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar85s.each do |bar85| %>
      <tr>
        <td><%= bar85.foo1 %></td>
        <td><%= bar85.foo2 %></td>
        <td><%= bar85.foo3 %></td>
        <td><%= bar85.foo4 %></td>
        <td><%= bar85.foo5 %></td>
        <td><%= link_to 'Show', bar85 %></td>
        <td><%= link_to 'Edit', edit_bar85_path(bar85) %></td>
        <td><%= link_to 'Destroy', bar85, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar85', new_bar85_path %>
