<p id="notice"><%= notice %></p>

<h1>Bar45s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar45s.each do |bar45| %>
      <tr>
        <td><%= bar45.foo1 %></td>
        <td><%= bar45.foo2 %></td>
        <td><%= bar45.foo3 %></td>
        <td><%= bar45.foo4 %></td>
        <td><%= bar45.foo5 %></td>
        <td><%= link_to 'Show', bar45 %></td>
        <td><%= link_to 'Edit', edit_bar45_path(bar45) %></td>
        <td><%= link_to 'Destroy', bar45, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar45', new_bar45_path %>
