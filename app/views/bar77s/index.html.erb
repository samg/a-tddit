<p id="notice"><%= notice %></p>

<h1>Bar77s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar77s.each do |bar77| %>
      <tr>
        <td><%= bar77.foo1 %></td>
        <td><%= bar77.foo2 %></td>
        <td><%= bar77.foo3 %></td>
        <td><%= bar77.foo4 %></td>
        <td><%= bar77.foo5 %></td>
        <td><%= link_to 'Show', bar77 %></td>
        <td><%= link_to 'Edit', edit_bar77_path(bar77) %></td>
        <td><%= link_to 'Destroy', bar77, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar77', new_bar77_path %>
