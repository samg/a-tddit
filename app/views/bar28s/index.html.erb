<h1>Bar28s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar28s.each do |bar28| %>
      <tr>
        <td><%= bar28.foo1 %></td>
        <td><%= bar28.foo2 %></td>
        <td><%= bar28.foo3 %></td>
        <td><%= bar28.foo4 %></td>
        <td><%= bar28.foo5 %></td>
        <td><%= link_to 'Show', bar28 %></td>
        <td><%= link_to 'Edit', edit_bar28_path(bar28) %></td>
        <td><%= link_to 'Destroy', bar28, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

