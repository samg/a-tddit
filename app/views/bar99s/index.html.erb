<p id="notice"><%= notice %></p>

<h1>Bar99s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar99s.each do |bar99| %>
      <tr>
        <td><%= bar99.foo1 %></td>
        <td><%= bar99.foo2 %></td>
        <td><%= bar99.foo3 %></td>
        <td><%= bar99.foo4 %></td>
        <td><%= bar99.foo5 %></td>
        <td><%= link_to 'Show', bar99 %></td>
        <td><%= link_to 'Edit', edit_bar99_path(bar99) %></td>
        <td><%= link_to 'Destroy', bar99, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar99', new_bar99_path %>
