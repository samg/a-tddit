<p id="notice"><%= notice %></p>

<h1>Bar4s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar4s.each do |bar4| %>
      <tr>
        <td><%= bar4.foo1 %></td>
        <td><%= bar4.foo2 %></td>
        <td><%= bar4.foo3 %></td>
        <td><%= bar4.foo4 %></td>
        <td><%= bar4.foo5 %></td>
        <td><%= link_to 'Show', bar4 %></td>
        <td><%= link_to 'Edit', edit_bar4_path(bar4) %></td>
        <td><%= link_to 'Destroy', bar4, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar4', new_bar4_path %>
