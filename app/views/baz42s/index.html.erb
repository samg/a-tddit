<p id="notice"><%= notice %></p>

<h1>Baz42s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz42s.each do |baz42| %>
      <tr>
        <td><%= baz42.foo1 %></td>
        <td><%= baz42.foo2 %></td>
        <td><%= baz42.foo3 %></td>
        <td><%= baz42.foo4 %></td>
        <td><%= baz42.foo5 %></td>
        <td><%= link_to 'Show', baz42 %></td>
        <td><%= link_to 'Edit', edit_baz42_path(baz42) %></td>
        <td><%= link_to 'Destroy', baz42, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz42', new_baz42_path %>
