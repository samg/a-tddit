<p id="notice"><%= notice %></p>

<h1>Bar7s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar7s.each do |bar7| %>
      <tr>
        <td><%= bar7.foo1 %></td>
        <td><%= bar7.foo2 %></td>
        <td><%= bar7.foo3 %></td>
        <td><%= bar7.foo4 %></td>
        <td><%= bar7.foo5 %></td>
        <td><%= link_to 'Show', bar7 %></td>
        <td><%= link_to 'Edit', edit_bar7_path(bar7) %></td>
        <td><%= link_to 'Destroy', bar7, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar7', new_bar7_path %>
