<p id="notice"><%= notice %></p>

<h1>Foo51s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo51s.each do |foo51| %>
      <tr>
        <td><%= foo51.foo1 %></td>
        <td><%= foo51.foo2 %></td>
        <td><%= foo51.foo3 %></td>
        <td><%= foo51.foo4 %></td>
        <td><%= foo51.foo5 %></td>
        <td><%= link_to 'Show', foo51 %></td>
        <td><%= link_to 'Edit', edit_foo51_path(foo51) %></td>
        <td><%= link_to 'Destroy', foo51, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

