<p id="notice"><%= notice %></p>

<h1>Foo72s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo72s.each do |foo72| %>
      <tr>
        <td><%= foo72.foo1 %></td>
        <td><%= foo72.foo2 %></td>
        <td><%= foo72.foo3 %></td>
        <td><%= foo72.foo4 %></td>
        <td><%= foo72.foo5 %></td>
        <td><%= link_to 'Show', foo72 %></td>
        <td><%= link_to 'Edit', edit_foo72_path(foo72) %></td>
        <td><%= link_to 'Destroy', foo72, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

