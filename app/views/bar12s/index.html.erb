<p id="notice"><%= notice %></p>

<h1>Bar12s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar12s.each do |bar12| %>
      <tr>
        <td><%= bar12.foo1 %></td>
        <td><%= bar12.foo2 %></td>
        <td><%= bar12.foo3 %></td>
        <td><%= bar12.foo4 %></td>
        <td><%= bar12.foo5 %></td>
        <td><%= link_to 'Show', bar12 %></td>
        <td><%= link_to 'Edit', edit_bar12_path(bar12) %></td>
        <td><%= link_to 'Destroy', bar12, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar12', new_bar12_path %>
