<p id="notice"><%= notice %></p>

<h1>Foo37s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo37s.each do |foo37| %>
      <tr>
        <td><%= foo37.foo1 %></td>
        <td><%= foo37.foo2 %></td>
        <td><%= foo37.foo3 %></td>
        <td><%= foo37.foo4 %></td>
        <td><%= foo37.foo5 %></td>
        <td><%= link_to 'Show', foo37 %></td>
        <td><%= link_to 'Edit', edit_foo37_path(foo37) %></td>
        <td><%= link_to 'Destroy', foo37, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foo37', new_foo37_path %>
