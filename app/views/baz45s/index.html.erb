<p id="notice"><%= notice %></p>

<h1>Baz45s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz45s.each do |baz45| %>
      <tr>
        <td><%= baz45.foo1 %></td>
        <td><%= baz45.foo2 %></td>
        <td><%= baz45.foo3 %></td>
        <td><%= baz45.foo4 %></td>
        <td><%= baz45.foo5 %></td>
        <td><%= link_to 'Show', baz45 %></td>
        <td><%= link_to 'Edit', edit_baz45_path(baz45) %></td>
        <td><%= link_to 'Destroy', baz45, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz45', new_baz45_path %>
