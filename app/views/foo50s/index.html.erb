<p id="notice"><%= notice %></p>

<h1>Foo50s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo50s.each do |foo50| %>
      <tr>
        <td><%= foo50.foo1 %></td>
        <td><%= foo50.foo2 %></td>
        <td><%= foo50.foo3 %></td>
        <td><%= foo50.foo4 %></td>
        <td><%= foo50.foo5 %></td>
        <td><%= link_to 'Show', foo50 %></td>
        <td><%= link_to 'Edit', edit_foo50_path(foo50) %></td>
        <td><%= link_to 'Destroy', foo50, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foo50', new_foo50_path %>
