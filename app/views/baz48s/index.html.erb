<p id="notice"><%= notice %></p>

<h1>Baz48s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz48s.each do |baz48| %>
      <tr>
        <td><%= baz48.foo1 %></td>
        <td><%= baz48.foo2 %></td>
        <td><%= baz48.foo3 %></td>
        <td><%= baz48.foo4 %></td>
        <td><%= baz48.foo5 %></td>
        <td><%= link_to 'Show', baz48 %></td>
        <td><%= link_to 'Edit', edit_baz48_path(baz48) %></td>
        <td><%= link_to 'Destroy', baz48, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

