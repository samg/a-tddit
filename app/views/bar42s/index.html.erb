<p id="notice"><%= notice %></p>

<h1>Bar42s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar42s.each do |bar42| %>
      <tr>
        <td><%= bar42.foo1 %></td>
        <td><%= bar42.foo2 %></td>
        <td><%= bar42.foo3 %></td>
        <td><%= bar42.foo4 %></td>
        <td><%= bar42.foo5 %></td>
        <td><%= link_to 'Show', bar42 %></td>
        <td><%= link_to 'Edit', edit_bar42_path(bar42) %></td>
        <td><%= link_to 'Destroy', bar42, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar42', new_bar42_path %>
