<p id="notice"><%= notice %></p>

<h1>Bar64s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar64s.each do |bar64| %>
      <tr>
        <td><%= bar64.foo1 %></td>
        <td><%= bar64.foo2 %></td>
        <td><%= bar64.foo3 %></td>
        <td><%= bar64.foo4 %></td>
        <td><%= bar64.foo5 %></td>
        <td><%= link_to 'Show', bar64 %></td>
        <td><%= link_to 'Edit', edit_bar64_path(bar64) %></td>
        <td><%= link_to 'Destroy', bar64, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar64', new_bar64_path %>
