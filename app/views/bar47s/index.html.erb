<p id="notice"><%= notice %></p>

<h1>Bar47s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar47s.each do |bar47| %>
      <tr>
        <td><%= bar47.foo1 %></td>
        <td><%= bar47.foo2 %></td>
        <td><%= bar47.foo3 %></td>
        <td><%= bar47.foo4 %></td>
        <td><%= bar47.foo5 %></td>
        <td><%= link_to 'Show', bar47 %></td>
        <td><%= link_to 'Edit', edit_bar47_path(bar47) %></td>
        <td><%= link_to 'Destroy', bar47, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

