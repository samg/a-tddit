<h1>Baz47s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz47s.each do |baz47| %>
      <tr>
        <td><%= baz47.foo1 %></td>
        <td><%= baz47.foo2 %></td>
        <td><%= baz47.foo3 %></td>
        <td><%= baz47.foo4 %></td>
        <td><%= baz47.foo5 %></td>
        <td><%= link_to 'Show', baz47 %></td>
        <td><%= link_to 'Edit', edit_baz47_path(baz47) %></td>
        <td><%= link_to 'Destroy', baz47, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

