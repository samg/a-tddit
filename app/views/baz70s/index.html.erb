<h1>Baz70s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz70s.each do |baz70| %>
      <tr>
        <td><%= baz70.foo1 %></td>
        <td><%= baz70.foo2 %></td>
        <td><%= baz70.foo3 %></td>
        <td><%= baz70.foo4 %></td>
        <td><%= baz70.foo5 %></td>
        <td><%= link_to 'Show', baz70 %></td>
        <td><%= link_to 'Edit', edit_baz70_path(baz70) %></td>
        <td><%= link_to 'Destroy', baz70, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

