<p id="notice"><%= notice %></p>

<h1>Foo15s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo15s.each do |foo15| %>
      <tr>
        <td><%= foo15.foo1 %></td>
        <td><%= foo15.foo2 %></td>
        <td><%= foo15.foo3 %></td>
        <td><%= foo15.foo4 %></td>
        <td><%= foo15.foo5 %></td>
        <td><%= link_to 'Show', foo15 %></td>
        <td><%= link_to 'Edit', edit_foo15_path(foo15) %></td>
        <td><%= link_to 'Destroy', foo15, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foo15', new_foo15_path %>
