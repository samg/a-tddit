<p id="notice"><%= notice %></p>

<h1>Foo38s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo38s.each do |foo38| %>
      <tr>
        <td><%= foo38.foo1 %></td>
        <td><%= foo38.foo2 %></td>
        <td><%= foo38.foo3 %></td>
        <td><%= foo38.foo4 %></td>
        <td><%= foo38.foo5 %></td>
        <td><%= link_to 'Show', foo38 %></td>
        <td><%= link_to 'Edit', edit_foo38_path(foo38) %></td>
        <td><%= link_to 'Destroy', foo38, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foo38', new_foo38_path %>
