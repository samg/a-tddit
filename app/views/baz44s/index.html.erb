<p id="notice"><%= notice %></p>

<h1>Baz44s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz44s.each do |baz44| %>
      <tr>
        <td><%= baz44.foo1 %></td>
        <td><%= baz44.foo2 %></td>
        <td><%= baz44.foo3 %></td>
        <td><%= baz44.foo4 %></td>
        <td><%= baz44.foo5 %></td>
        <td><%= link_to 'Show', baz44 %></td>
        <td><%= link_to 'Edit', edit_baz44_path(baz44) %></td>
        <td><%= link_to 'Destroy', baz44, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

