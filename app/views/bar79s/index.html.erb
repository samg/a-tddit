<p id="notice"><%= notice %></p>

<h1>Bar79s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar79s.each do |bar79| %>
      <tr>
        <td><%= bar79.foo1 %></td>
        <td><%= bar79.foo2 %></td>
        <td><%= bar79.foo3 %></td>
        <td><%= bar79.foo4 %></td>
        <td><%= bar79.foo5 %></td>
        <td><%= link_to 'Show', bar79 %></td>
        <td><%= link_to 'Edit', edit_bar79_path(bar79) %></td>
        <td><%= link_to 'Destroy', bar79, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar79', new_bar79_path %>
