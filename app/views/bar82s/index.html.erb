<p id="notice"><%= notice %></p>

<h1>Bar82s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar82s.each do |bar82| %>
      <tr>
        <td><%= bar82.foo1 %></td>
        <td><%= bar82.foo2 %></td>
        <td><%= bar82.foo3 %></td>
        <td><%= bar82.foo4 %></td>
        <td><%= bar82.foo5 %></td>
        <td><%= link_to 'Show', bar82 %></td>
        <td><%= link_to 'Edit', edit_bar82_path(bar82) %></td>
        <td><%= link_to 'Destroy', bar82, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar82', new_bar82_path %>
