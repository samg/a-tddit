<p id="notice"><%= notice %></p>

<h1>Bar78s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar78s.each do |bar78| %>
      <tr>
        <td><%= bar78.foo1 %></td>
        <td><%= bar78.foo2 %></td>
        <td><%= bar78.foo3 %></td>
        <td><%= bar78.foo4 %></td>
        <td><%= bar78.foo5 %></td>
        <td><%= link_to 'Show', bar78 %></td>
        <td><%= link_to 'Edit', edit_bar78_path(bar78) %></td>
        <td><%= link_to 'Destroy', bar78, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar78', new_bar78_path %>
