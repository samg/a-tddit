<p id="notice"><%= notice %></p>

<h1>Bar6s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar6s.each do |bar6| %>
      <tr>
        <td><%= bar6.foo1 %></td>
        <td><%= bar6.foo2 %></td>
        <td><%= bar6.foo3 %></td>
        <td><%= bar6.foo4 %></td>
        <td><%= bar6.foo5 %></td>
        <td><%= link_to 'Show', bar6 %></td>
        <td><%= link_to 'Edit', edit_bar6_path(bar6) %></td>
        <td><%= link_to 'Destroy', bar6, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar6', new_bar6_path %>
