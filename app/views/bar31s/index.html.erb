<p id="notice"><%= notice %></p>

<h1>Bar31s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar31s.each do |bar31| %>
      <tr>
        <td><%= bar31.foo1 %></td>
        <td><%= bar31.foo2 %></td>
        <td><%= bar31.foo3 %></td>
        <td><%= bar31.foo4 %></td>
        <td><%= bar31.foo5 %></td>
        <td><%= link_to 'Show', bar31 %></td>
        <td><%= link_to 'Edit', edit_bar31_path(bar31) %></td>
        <td><%= link_to 'Destroy', bar31, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar31', new_bar31_path %>
