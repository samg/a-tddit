<p id="notice"><%= notice %></p>

<h1>Bar98s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar98s.each do |bar98| %>
      <tr>
        <td><%= bar98.foo1 %></td>
        <td><%= bar98.foo2 %></td>
        <td><%= bar98.foo3 %></td>
        <td><%= bar98.foo4 %></td>
        <td><%= bar98.foo5 %></td>
        <td><%= link_to 'Show', bar98 %></td>
        <td><%= link_to 'Edit', edit_bar98_path(bar98) %></td>
        <td><%= link_to 'Destroy', bar98, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar98', new_bar98_path %>
