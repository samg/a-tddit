<h1>Bar27s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar27s.each do |bar27| %>
      <tr>
        <td><%= bar27.foo1 %></td>
        <td><%= bar27.foo2 %></td>
        <td><%= bar27.foo3 %></td>
        <td><%= bar27.foo4 %></td>
        <td><%= bar27.foo5 %></td>
        <td><%= link_to 'Show', bar27 %></td>
        <td><%= link_to 'Edit', edit_bar27_path(bar27) %></td>
        <td><%= link_to 'Destroy', bar27, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

