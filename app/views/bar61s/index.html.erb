<p id="notice"><%= notice %></p>

<h1>Bar61s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar61s.each do |bar61| %>
      <tr>
        <td><%= bar61.foo1 %></td>
        <td><%= bar61.foo2 %></td>
        <td><%= bar61.foo3 %></td>
        <td><%= bar61.foo4 %></td>
        <td><%= bar61.foo5 %></td>
        <td><%= link_to 'Show', bar61 %></td>
        <td><%= link_to 'Edit', edit_bar61_path(bar61) %></td>
        <td><%= link_to 'Destroy', bar61, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar61', new_bar61_path %>
