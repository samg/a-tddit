<p id="notice"><%= notice %></p>

<h1>Bar88s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar88s.each do |bar88| %>
      <tr>
        <td><%= bar88.foo1 %></td>
        <td><%= bar88.foo2 %></td>
        <td><%= bar88.foo3 %></td>
        <td><%= bar88.foo4 %></td>
        <td><%= bar88.foo5 %></td>
        <td><%= link_to 'Show', bar88 %></td>
        <td><%= link_to 'Edit', edit_bar88_path(bar88) %></td>
        <td><%= link_to 'Destroy', bar88, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar88', new_bar88_path %>
