<p id="notice"><%= notice %></p>

<h1>Bar20s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar20s.each do |bar20| %>
      <tr>
        <td><%= bar20.foo1 %></td>
        <td><%= bar20.foo2 %></td>
        <td><%= bar20.foo3 %></td>
        <td><%= bar20.foo4 %></td>
        <td><%= bar20.foo5 %></td>
        <td><%= link_to 'Show', bar20 %></td>
        <td><%= link_to 'Edit', edit_bar20_path(bar20) %></td>
        <td><%= link_to 'Destroy', bar20, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar20', new_bar20_path %>
