<p id="notice"><%= notice %></p>

<h1>Baz21s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz21s.each do |baz21| %>
      <tr>
        <td><%= baz21.foo1 %></td>
        <td><%= baz21.foo2 %></td>
        <td><%= baz21.foo3 %></td>
        <td><%= baz21.foo4 %></td>
        <td><%= baz21.foo5 %></td>
        <td><%= link_to 'Show', baz21 %></td>
        <td><%= link_to 'Edit', edit_baz21_path(baz21) %></td>
        <td><%= link_to 'Destroy', baz21, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

