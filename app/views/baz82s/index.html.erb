<h1>Baz82s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz82s.each do |baz82| %>
      <tr>
        <td><%= baz82.foo1 %></td>
        <td><%= baz82.foo2 %></td>
        <td><%= baz82.foo3 %></td>
        <td><%= baz82.foo4 %></td>
        <td><%= baz82.foo5 %></td>
        <td><%= link_to 'Show', baz82 %></td>
        <td><%= link_to 'Edit', edit_baz82_path(baz82) %></td>
        <td><%= link_to 'Destroy', baz82, method: :delete, data: { confirm: 'Are you sure?' } %></td>
