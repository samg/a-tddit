<p id="notice"><%= notice %></p>

<h1>Bar13s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar13s.each do |bar13| %>
      <tr>
        <td><%= bar13.foo1 %></td>
        <td><%= bar13.foo2 %></td>
        <td><%= bar13.foo3 %></td>
        <td><%= bar13.foo4 %></td>
        <td><%= bar13.foo5 %></td>
        <td><%= link_to 'Show', bar13 %></td>
        <td><%= link_to 'Edit', edit_bar13_path(bar13) %></td>
        <td><%= link_to 'Destroy', bar13, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

