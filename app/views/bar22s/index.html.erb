<p id="notice"><%= notice %></p>

<h1>Bar22s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar22s.each do |bar22| %>
      <tr>
        <td><%= bar22.foo1 %></td>
        <td><%= bar22.foo2 %></td>
        <td><%= bar22.foo3 %></td>
        <td><%= bar22.foo4 %></td>
        <td><%= bar22.foo5 %></td>
        <td><%= link_to 'Show', bar22 %></td>
        <td><%= link_to 'Edit', edit_bar22_path(bar22) %></td>
        <td><%= link_to 'Destroy', bar22, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

