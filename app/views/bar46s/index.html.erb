<p id="notice"><%= notice %></p>

<h1>Bar46s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar46s.each do |bar46| %>
      <tr>
        <td><%= bar46.foo1 %></td>
        <td><%= bar46.foo2 %></td>
        <td><%= bar46.foo3 %></td>
        <td><%= bar46.foo4 %></td>
        <td><%= bar46.foo5 %></td>
        <td><%= link_to 'Show', bar46 %></td>
        <td><%= link_to 'Edit', edit_bar46_path(bar46) %></td>
        <td><%= link_to 'Destroy', bar46, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar46', new_bar46_path %>
