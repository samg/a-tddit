<p id="notice"><%= notice %></p>

<h1>Foo3s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo3s.each do |foo3| %>
      <tr>
        <td><%= foo3.foo1 %></td>
        <td><%= foo3.foo2 %></td>
        <td><%= foo3.foo3 %></td>
        <td><%= foo3.foo4 %></td>
        <td><%= foo3.foo5 %></td>
        <td><%= link_to 'Show', foo3 %></td>
        <td><%= link_to 'Edit', edit_foo3_path(foo3) %></td>
        <td><%= link_to 'Destroy', foo3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foo3', new_foo3_path %>
