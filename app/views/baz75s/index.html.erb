<p id="notice"><%= notice %></p>

<h1>Baz75s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz75s.each do |baz75| %>
      <tr>
        <td><%= baz75.foo1 %></td>
        <td><%= baz75.foo2 %></td>
        <td><%= baz75.foo3 %></td>
        <td><%= baz75.foo4 %></td>
        <td><%= baz75.foo5 %></td>
        <td><%= link_to 'Show', baz75 %></td>
        <td><%= link_to 'Edit', edit_baz75_path(baz75) %></td>
        <td><%= link_to 'Destroy', baz75, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz75', new_baz75_path %>
