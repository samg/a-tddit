<p id="notice"><%= notice %></p>

<h1>Baz40s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz40s.each do |baz40| %>
      <tr>
        <td><%= baz40.foo1 %></td>
        <td><%= baz40.foo2 %></td>
        <td><%= baz40.foo3 %></td>
        <td><%= baz40.foo4 %></td>
        <td><%= baz40.foo5 %></td>
        <td><%= link_to 'Show', baz40 %></td>
        <td><%= link_to 'Edit', edit_baz40_path(baz40) %></td>
        <td><%= link_to 'Destroy', baz40, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz40', new_baz40_path %>
