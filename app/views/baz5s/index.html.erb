<p id="notice"><%= notice %></p>

<h1>Baz5s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz5s.each do |baz5| %>
      <tr>
        <td><%= baz5.foo1 %></td>
        <td><%= baz5.foo2 %></td>
        <td><%= baz5.foo3 %></td>
        <td><%= baz5.foo4 %></td>
        <td><%= baz5.foo5 %></td>
        <td><%= link_to 'Show', baz5 %></td>
        <td><%= link_to 'Edit', edit_baz5_path(baz5) %></td>
        <td><%= link_to 'Destroy', baz5, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

