<p id="notice"><%= notice %></p>

<h1>Bar52s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar52s.each do |bar52| %>
      <tr>
        <td><%= bar52.foo1 %></td>
        <td><%= bar52.foo2 %></td>
        <td><%= bar52.foo3 %></td>
        <td><%= bar52.foo4 %></td>
        <td><%= bar52.foo5 %></td>
        <td><%= link_to 'Show', bar52 %></td>
        <td><%= link_to 'Edit', edit_bar52_path(bar52) %></td>
        <td><%= link_to 'Destroy', bar52, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar52', new_bar52_path %>
