<p id="notice"><%= notice %></p>

<h1>Bar26s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar26s.each do |bar26| %>
      <tr>
        <td><%= bar26.foo1 %></td>
        <td><%= bar26.foo2 %></td>
        <td><%= bar26.foo3 %></td>
        <td><%= bar26.foo4 %></td>
        <td><%= bar26.foo5 %></td>
        <td><%= link_to 'Show', bar26 %></td>
        <td><%= link_to 'Edit', edit_bar26_path(bar26) %></td>
        <td><%= link_to 'Destroy', bar26, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar26', new_bar26_path %>
