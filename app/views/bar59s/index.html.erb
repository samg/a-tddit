<p id="notice"><%= notice %></p>

<h1>Bar59s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar59s.each do |bar59| %>
      <tr>
        <td><%= bar59.foo1 %></td>
        <td><%= bar59.foo2 %></td>
        <td><%= bar59.foo3 %></td>
        <td><%= bar59.foo4 %></td>
        <td><%= bar59.foo5 %></td>
        <td><%= link_to 'Show', bar59 %></td>
        <td><%= link_to 'Edit', edit_bar59_path(bar59) %></td>
        <td><%= link_to 'Destroy', bar59, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

