<p id="notice"><%= notice %></p>

<h1>Baz64s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz64s.each do |baz64| %>
      <tr>
        <td><%= baz64.foo1 %></td>
        <td><%= baz64.foo2 %></td>
        <td><%= baz64.foo3 %></td>
        <td><%= baz64.foo4 %></td>
        <td><%= baz64.foo5 %></td>
        <td><%= link_to 'Show', baz64 %></td>
        <td><%= link_to 'Edit', edit_baz64_path(baz64) %></td>
        <td><%= link_to 'Destroy', baz64, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz64', new_baz64_path %>
