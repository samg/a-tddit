<p id="notice"><%= notice %></p>

<h1>Baz53s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz53s.each do |baz53| %>
      <tr>
        <td><%= baz53.foo1 %></td>
        <td><%= baz53.foo2 %></td>
        <td><%= baz53.foo3 %></td>
        <td><%= baz53.foo4 %></td>
        <td><%= baz53.foo5 %></td>
        <td><%= link_to 'Show', baz53 %></td>
        <td><%= link_to 'Edit', edit_baz53_path(baz53) %></td>
        <td><%= link_to 'Destroy', baz53, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

