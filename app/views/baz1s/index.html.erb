<p id="notice"><%= notice %></p>

<h1>Baz1s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz1s.each do |baz1| %>
      <tr>
        <td><%= baz1.foo1 %></td>
        <td><%= baz1.foo2 %></td>
        <td><%= baz1.foo3 %></td>
        <td><%= baz1.foo4 %></td>
        <td><%= baz1.foo5 %></td>
        <td><%= link_to 'Show', baz1 %></td>
        <td><%= link_to 'Edit', edit_baz1_path(baz1) %></td>
        <td><%= link_to 'Destroy', baz1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz1', new_baz1_path %>
