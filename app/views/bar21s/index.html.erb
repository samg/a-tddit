<p id="notice"><%= notice %></p>

<h1>Bar21s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar21s.each do |bar21| %>
      <tr>
        <td><%= bar21.foo1 %></td>
        <td><%= bar21.foo2 %></td>
        <td><%= bar21.foo3 %></td>
        <td><%= bar21.foo4 %></td>
        <td><%= bar21.foo5 %></td>
        <td><%= link_to 'Show', bar21 %></td>
        <td><%= link_to 'Edit', edit_bar21_path(bar21) %></td>
        <td><%= link_to 'Destroy', bar21, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar21', new_bar21_path %>
