<p id="notice"><%= notice %></p>

<h1>Bar100s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar100s.each do |bar100| %>
      <tr>
        <td><%= bar100.foo1 %></td>
        <td><%= bar100.foo2 %></td>
        <td><%= bar100.foo3 %></td>
        <td><%= bar100.foo4 %></td>
        <td><%= bar100.foo5 %></td>
        <td><%= link_to 'Show', bar100 %></td>
        <td><%= link_to 'Edit', edit_bar100_path(bar100) %></td>
        <td><%= link_to 'Destroy', bar100, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar100', new_bar100_path %>
