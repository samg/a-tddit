<p id="notice"><%= notice %></p>

<h1>Baz46s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz46s.each do |baz46| %>
      <tr>
        <td><%= baz46.foo1 %></td>
        <td><%= baz46.foo2 %></td>
        <td><%= baz46.foo3 %></td>
        <td><%= baz46.foo4 %></td>
        <td><%= baz46.foo5 %></td>
        <td><%= link_to 'Show', baz46 %></td>
        <td><%= link_to 'Edit', edit_baz46_path(baz46) %></td>
        <td><%= link_to 'Destroy', baz46, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

