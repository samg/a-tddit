<p id="notice"><%= notice %></p>

<h1>Baz99s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz99s.each do |baz99| %>
      <tr>
        <td><%= baz99.foo1 %></td>
        <td><%= baz99.foo2 %></td>
        <td><%= baz99.foo3 %></td>
        <td><%= baz99.foo4 %></td>
        <td><%= baz99.foo5 %></td>
        <td><%= link_to 'Show', baz99 %></td>
        <td><%= link_to 'Edit', edit_baz99_path(baz99) %></td>
        <td><%= link_to 'Destroy', baz99, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz99', new_baz99_path %>
