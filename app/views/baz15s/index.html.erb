<h1>Baz15s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz15s.each do |baz15| %>
      <tr>
        <td><%= baz15.foo1 %></td>
        <td><%= baz15.foo2 %></td>
        <td><%= baz15.foo3 %></td>
        <td><%= baz15.foo4 %></td>
        <td><%= baz15.foo5 %></td>
        <td><%= link_to 'Show', baz15 %></td>
        <td><%= link_to 'Edit', edit_baz15_path(baz15) %></td>
        <td><%= link_to 'Destroy', baz15, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

