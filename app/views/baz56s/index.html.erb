<h1>Baz56s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz56s.each do |baz56| %>
      <tr>
        <td><%= baz56.foo1 %></td>
        <td><%= baz56.foo2 %></td>
        <td><%= baz56.foo3 %></td>
        <td><%= baz56.foo4 %></td>
        <td><%= baz56.foo5 %></td>
        <td><%= link_to 'Show', baz56 %></td>
        <td><%= link_to 'Edit', edit_baz56_path(baz56) %></td>
        <td><%= link_to 'Destroy', baz56, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

