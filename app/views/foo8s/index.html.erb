<p id="notice"><%= notice %></p>

<h1>Foo8s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo8s.each do |foo8| %>
      <tr>
        <td><%= foo8.foo1 %></td>
        <td><%= foo8.foo2 %></td>
        <td><%= foo8.foo3 %></td>
        <td><%= foo8.foo4 %></td>
        <td><%= foo8.foo5 %></td>
        <td><%= link_to 'Show', foo8 %></td>
        <td><%= link_to 'Edit', edit_foo8_path(foo8) %></td>
        <td><%= link_to 'Destroy', foo8, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

