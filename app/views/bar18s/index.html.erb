<p id="notice"><%= notice %></p>

<h1>Bar18s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar18s.each do |bar18| %>
      <tr>
        <td><%= bar18.foo1 %></td>
        <td><%= bar18.foo2 %></td>
        <td><%= bar18.foo3 %></td>
        <td><%= bar18.foo4 %></td>
        <td><%= bar18.foo5 %></td>
        <td><%= link_to 'Show', bar18 %></td>
        <td><%= link_to 'Edit', edit_bar18_path(bar18) %></td>
        <td><%= link_to 'Destroy', bar18, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar18', new_bar18_path %>
