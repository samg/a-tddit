<p id="notice"><%= notice %></p>

<h1>Bar17s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar17s.each do |bar17| %>
      <tr>
        <td><%= bar17.foo1 %></td>
        <td><%= bar17.foo2 %></td>
        <td><%= bar17.foo3 %></td>
        <td><%= bar17.foo4 %></td>
        <td><%= bar17.foo5 %></td>
        <td><%= link_to 'Show', bar17 %></td>
        <td><%= link_to 'Edit', edit_bar17_path(bar17) %></td>
        <td><%= link_to 'Destroy', bar17, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar17', new_bar17_path %>
