<p id="notice"><%= notice %></p>

<h1>Bar39s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar39s.each do |bar39| %>
      <tr>
        <td><%= bar39.foo1 %></td>
        <td><%= bar39.foo2 %></td>
        <td><%= bar39.foo3 %></td>
        <td><%= bar39.foo4 %></td>
        <td><%= bar39.foo5 %></td>
        <td><%= link_to 'Show', bar39 %></td>
        <td><%= link_to 'Edit', edit_bar39_path(bar39) %></td>
        <td><%= link_to 'Destroy', bar39, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar39', new_bar39_path %>
