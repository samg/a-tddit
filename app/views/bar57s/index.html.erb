<p id="notice"><%= notice %></p>

<h1>Bar57s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar57s.each do |bar57| %>
      <tr>
        <td><%= bar57.foo1 %></td>
        <td><%= bar57.foo2 %></td>
        <td><%= bar57.foo3 %></td>
        <td><%= bar57.foo4 %></td>
        <td><%= bar57.foo5 %></td>
        <td><%= link_to 'Show', bar57 %></td>
        <td><%= link_to 'Edit', edit_bar57_path(bar57) %></td>
        <td><%= link_to 'Destroy', bar57, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar57', new_bar57_path %>
