<p id="notice"><%= notice %></p>

<h1>Baz100s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz100s.each do |baz100| %>
      <tr>
        <td><%= baz100.foo1 %></td>
        <td><%= baz100.foo2 %></td>
        <td><%= baz100.foo3 %></td>
        <td><%= baz100.foo4 %></td>
        <td><%= baz100.foo5 %></td>
        <td><%= link_to 'Show', baz100 %></td>
        <td><%= link_to 'Edit', edit_baz100_path(baz100) %></td>
        <td><%= link_to 'Destroy', baz100, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz100', new_baz100_path %>
