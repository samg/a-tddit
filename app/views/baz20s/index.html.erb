<p id="notice"><%= notice %></p>

<h1>Baz20s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz20s.each do |baz20| %>
      <tr>
        <td><%= baz20.foo1 %></td>
        <td><%= baz20.foo2 %></td>
        <td><%= baz20.foo3 %></td>
        <td><%= baz20.foo4 %></td>
        <td><%= baz20.foo5 %></td>
        <td><%= link_to 'Show', baz20 %></td>
        <td><%= link_to 'Edit', edit_baz20_path(baz20) %></td>
        <td><%= link_to 'Destroy', baz20, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz20', new_baz20_path %>
