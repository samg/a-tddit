<p id="notice"><%= notice %></p>

<h1>Bar8s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar8s.each do |bar8| %>
      <tr>
        <td><%= bar8.foo1 %></td>
        <td><%= bar8.foo2 %></td>
        <td><%= bar8.foo3 %></td>
        <td><%= bar8.foo4 %></td>
        <td><%= bar8.foo5 %></td>
        <td><%= link_to 'Show', bar8 %></td>
        <td><%= link_to 'Edit', edit_bar8_path(bar8) %></td>
        <td><%= link_to 'Destroy', bar8, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar8', new_bar8_path %>
