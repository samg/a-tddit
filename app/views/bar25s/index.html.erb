<p id="notice"><%= notice %></p>

<h1>Bar25s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar25s.each do |bar25| %>
      <tr>
        <td><%= bar25.foo1 %></td>
        <td><%= bar25.foo2 %></td>
        <td><%= bar25.foo3 %></td>
        <td><%= bar25.foo4 %></td>
        <td><%= bar25.foo5 %></td>
        <td><%= link_to 'Show', bar25 %></td>
        <td><%= link_to 'Edit', edit_bar25_path(bar25) %></td>
        <td><%= link_to 'Destroy', bar25, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar25', new_bar25_path %>
