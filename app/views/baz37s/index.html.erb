<p id="notice"><%= notice %></p>

<h1>Baz37s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz37s.each do |baz37| %>
      <tr>
        <td><%= baz37.foo1 %></td>
        <td><%= baz37.foo2 %></td>
        <td><%= baz37.foo3 %></td>
        <td><%= baz37.foo4 %></td>
        <td><%= baz37.foo5 %></td>
        <td><%= link_to 'Show', baz37 %></td>
        <td><%= link_to 'Edit', edit_baz37_path(baz37) %></td>
        <td><%= link_to 'Destroy', baz37, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

