<p id="notice"><%= notice %></p>

<h1>Bar80s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar80s.each do |bar80| %>
      <tr>
        <td><%= bar80.foo1 %></td>
        <td><%= bar80.foo2 %></td>
        <td><%= bar80.foo3 %></td>
        <td><%= bar80.foo4 %></td>
        <td><%= bar80.foo5 %></td>
        <td><%= link_to 'Show', bar80 %></td>
        <td><%= link_to 'Edit', edit_bar80_path(bar80) %></td>
        <td><%= link_to 'Destroy', bar80, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar80', new_bar80_path %>
