<p id="notice"><%= notice %></p>

<h1>Baz85s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz85s.each do |baz85| %>
      <tr>
        <td><%= baz85.foo1 %></td>
        <td><%= baz85.foo2 %></td>
        <td><%= baz85.foo3 %></td>
        <td><%= baz85.foo4 %></td>
        <td><%= baz85.foo5 %></td>
        <td><%= link_to 'Show', baz85 %></td>
        <td><%= link_to 'Edit', edit_baz85_path(baz85) %></td>
        <td><%= link_to 'Destroy', baz85, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz85', new_baz85_path %>
