<p id="notice"><%= notice %></p>

<h1>Foo20s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo20s.each do |foo20| %>
      <tr>
        <td><%= foo20.foo1 %></td>
        <td><%= foo20.foo2 %></td>
        <td><%= foo20.foo3 %></td>
        <td><%= foo20.foo4 %></td>
        <td><%= foo20.foo5 %></td>
        <td><%= link_to 'Show', foo20 %></td>
        <td><%= link_to 'Edit', edit_foo20_path(foo20) %></td>
        <td><%= link_to 'Destroy', foo20, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foo20', new_foo20_path %>
