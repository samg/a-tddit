<p id="notice"><%= notice %></p>

<h1>Foo1s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foo1s.each do |foo1| %>
      <tr>
        <td><%= foo1.foo1 %></td>
        <td><%= foo1.foo2 %></td>
        <td><%= foo1.foo3 %></td>
        <td><%= foo1.foo4 %></td>
        <td><%= foo1.foo5 %></td>
        <td><%= link_to 'Show', foo1 %></td>
        <td><%= link_to 'Edit', edit_foo1_path(foo1) %></td>
        <td><%= link_to 'Destroy', foo1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foo1', new_foo1_path %>
