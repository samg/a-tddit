<p id="notice"><%= notice %></p>

<h1>Baz52s</h1>

<table>
  <thead>
    <tr>
      <th>Foo1</th>
      <th>Foo2</th>
      <th>Foo3</th>
      <th>Foo4</th>
      <th>Foo5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baz52s.each do |baz52| %>
      <tr>
        <td><%= baz52.foo1 %></td>
        <td><%= baz52.foo2 %></td>
        <td><%= baz52.foo3 %></td>
        <td><%= baz52.foo4 %></td>
        <td><%= baz52.foo5 %></td>
        <td><%= link_to 'Show', baz52 %></td>
        <td><%= link_to 'Edit', edit_baz52_path(baz52) %></td>
        <td><%= link_to 'Destroy', baz52, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Baz52', new_baz52_path %>
